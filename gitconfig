[default]
    push = matching
[color]
    ui = auto
[alias]
    s = status
    sa = status --ignored
    co = checkout
    a = add
    aa = add --all
    c = commit
    ca = commit -a
    cm = commit -m
    cma = commit -a -m
    cam = commit --amend
    ps = push
    pl = pull
    cl = clone
    t = tag
    ra = remote add
    rr = remote rm
    b = branch
    br = branch -r
    ba = branch -a
    l = log -2
    t = log --graph --pretty=format:'%C(yellow)%h%Creset%C(auto)%d%Creset %s %Cblue(%an)%Creset %Cgreen(%cr)%Creset' --abbrev-commit --all
    d = diff
    dc = diff --cached
    f = fetch
    m = merge
    su = submodule foreach git pull origin master